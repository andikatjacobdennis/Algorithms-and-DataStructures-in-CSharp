<Window x:Class="ComplexityGraphs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        Title="Complexity Graphs" Height="600" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Time Complexity">
                <oxy:PlotView Name="TimePlotView" />
            </TabItem>
            <TabItem Header="Space Complexity">
                <oxy:PlotView Name="SpacePlotView" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
